{
    "manifest_version": 2,
    "name": "Mail manager",
    "version": "1.0",
    "description": "helps to manage mails on gmail.com",
    "icons": {
        "48": "./icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.mail.google.com/*"
            ],
            "js": [
                "./content_script.js"
            ]
        }
    ]
}